input = """"""
def first():
    i = 3
    while i < len(input):
        new_set = set([*input[i-3:i+1]])
        if len(new_set) == 4:
            break
        i += 1
    print(i+1)

def second():
    i = 13
    while i < len(input):
        new_set = set([*input[i-13:i+1]])
        if len(new_set) == 14:
            break
        i += 1
    print(i+1)
first()
second()
